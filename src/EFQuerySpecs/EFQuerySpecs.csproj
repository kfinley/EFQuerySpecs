<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Description>EFQuerySpecs is a lightweight Domain Driven Design Specification pattern implementation for Entity Framework Core.</Description>
    <PackageId>EFQuerySpecs</PackageId>
    <Title>EFQuerySpecs</Title>
    <Copyright>Â© 2021 Kyle Finley</Copyright>
    <PackageTags>Entity-Framework </PackageTags>
    <Version>5.0.7</Version>
    <Authors>Kyle Finley</Authors>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/kfinley/EFQuerySpecs</PackageProjectUrl>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/kfinley/EFQuerySpecs</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.7" />
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="../../readme.txt" Pack="true" PackagePath="."/>
    <None Include="../../README.md" Pack="true" PackagePath="." />
    <None Include="../../LICENSE.txt" Pack="true" PackagePath="." />
  </ItemGroup>

</Project>
